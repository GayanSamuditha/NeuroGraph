<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jiashuwu.neurograph">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_NOTIFICATION_POLICY" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".WelcomeActivity"
            android:configChanges="locale" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SettingPageActivity"
            android:configChanges="locale" />
        <activity
            android:name=".InformationCollectionActivity"
            android:label="@string/title_activity_information_collection"
            android:theme="@style/AppTheme.NoActionBar"
            android:configChanges="locale" />
        <activity
            android:name=".TestSelectionActivity"
            android:label="@string/title_activity_test_selection"
            android:theme="@style/AppTheme.NoActionBar"
            android:configChanges="locale" />
        <activity
            android:name=".ImageSelectionActivity"
            android:label="@string/title_activity_image_selection"
            android:theme="@style/AppTheme.NoActionBar"
            android:configChanges="locale" />
        <activity android:name=".DurationSelectionActivity"
            android:configChanges="locale" />
        <activity
            android:name=".StaticBackgroundTestActivity"
            android:screenOrientation="landscape"
            android:configChanges="locale" />
        <activity
            android:name=".StaticCornerBackgroundTestActivity"
            android:screenOrientation="landscape"
            android:configChanges="locale" />
        <activity
            android:name=".DynamicBlankBackgroundTestActivity"
            android:screenOrientation="landscape"
            android:configChanges="locale" />
        <activity
            android:name=".DynamicShowBackgroundActivity"
            android:screenOrientation="landscape"
            android:configChanges="locale" />
        <activity
            android:name=".ThankYouActivity"
            android:screenOrientation="landscape"
            android:configChanges="locale" />
        <activity
            android:name=".DynamicSeasonalBackgroundTestActivity"
            android:screenOrientation="landscape"
            android:configChanges="locale" />
        <activity
            android:name=".DataListActivity"
            android:label="@string/title_activity_data_list"
            android:theme="@style/AppTheme.NoActionBar"
            android:configChanges="locale" />
        <activity
            android:name=".TestDetailScrollingActivity"
            android:label="@string/title_activity_test_detail_scrolling"
            android:theme="@style/AppTheme.NoActionBar"
            android:configChanges="locale" />
        <activity android:name=".TransferActivity" />
        <activity android:name=".SendDataEmailActivity"
            android:configChanges="locale" ></activity>
    </application>


</manifest>